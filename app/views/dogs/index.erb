<h1>All Dogs</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Breed</th>
      <th scope="col">Age</th>
    </tr>
  </thead>
  <tbody>
  <% r_count = 0 %>
  <% @dogs.each do |dog| %>
    <tr>
      <th scope="row"><%= r_count += 1 %></th>
      <td><%= link_to dog.name, dog_path(dog) %></td>
      <td><%= dog.breed %></td>
      <td><%= dog.age %></td>
    </tr>
    <%end%>
  </tbody>
</table>